/**app.wxss**/
@import "./component/CusToast/CusToast.wxss";

page {
  height: 100%;
}

.page {
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-top: 15rpx;
  padding-bottom: 200rpx;
  box-sizing: border-box;
  background-image: url('http://xcxcy.oss-cn-hangzhou.aliyuncs.com/cycck/icon/cy_beijing.jpg');
  background-size: 100% 100%;
  background-color: #ecb2d2;
}

.bc_green {
  background-color: #09bb07;
}

.bc_red {
  background-color: #f76260;
}

.bc_blue {
  background-color: #10aeff;
}

.bc_yellow {
  background-color: #ffbe00;
}

.bc_gray {
  background-color: #c9c9c9;
}

.fixed {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

.absolute {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

/*星星消失  */

.start_dx {
  animation: starta 0.8s linear;
  animation-fill-mode: forwards;
  animation-delay: 1950ms;
}

@keyframes starta {
  0% {
    opacity: 1;
    transform: scale(1);
  }

  100% {
    opacity: 0;
    transform: scale(5);
  }
}

/*星星全部渐显  */

.start_qbjx {
  animation: startq 0.8s linear;
  animation-fill-mode: forwards;
  animation-delay: 2750ms;
}

@keyframes startq {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

/*星星全部渐失  */

.start_qbxs {
  animation: startw 0.8s linear;
  animation-fill-mode: forwards;
  animation-delay: 1950ms;
}

@keyframes startw {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
  }
}

/*星星显示  */

.start_xs {
  animation: startb 0.8s linear;
  animation-fill-mode: forwards;
  animation-delay: 1950ms;
}

@keyframes startb {
  0% {
    opacity: 0;
    transform: scale(8);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

.ignoreEvt {
  pointer-events: none;
}

/*两行省略*/

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.new_frame {
  position: absolute;
  left: 0;
  right: 0;
}

.navigator-hover {
  background-color: rgba(0, 0, 0, 0);
  opacity: 1;
}

/*一行省略*/

.one_line {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*图片盒子*/

.icon_box {
  display: flex;
  justify-content: center;
  align-items: center;
}

.photo_img {
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
  border: 8rpx solid #34b7f7;
}

.user_photo {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
  border: 6rpx solid #37b9ff;
  box-sizing: border-box;
  overflow: hidden;
}

.box_shadow {
  box-shadow: 0px 6rpx 20rpx 6rpx #ddd;
  /*box-shadow: 0 -4rpx 6rpx 6rpx #eee;
  box-shadow: 水平阴影 垂直阴影 模糊距离 阴影大小 阴影颜色  内部阴影;*/
}

/*取消滑动条样式*/

::-webkit-scrollbar {
  width: 0rpx;
  height: 0rpx;
  /* color: red; */
}

::-webkit-scrollbar-track {
  width: 0rpx;
  height: 0rpx;
  /* color: red; */
}

/*星*/

.good_star {
  width: 30rpx;
  height: 30rpx;
}

/*背景图片*/

.bg {
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

image {
  margin-bottom: 0;
}

.remove_roll_car {
  -webkit-overflow-scrolling: touch;
}

