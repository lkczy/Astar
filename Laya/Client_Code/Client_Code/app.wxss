/*圆形进度  */

.app_circle_box {
  width: 88rpx;
  height: 88rpx;
  background-color: #f90;
  background: #d7d7d7;
  border-radius: 50%;
  position: relative;
}

/*   */

.app_circle_left_child, .app_circle_right_child {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  top: 0px;
  left: 0px;
}

/*  */

.app_circle_left, .app_circle_right {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  border-radius: 50%;
  top: 0px;
  left: 0px;
  /* background: #d7d7d7; */
  background-color: #f90;
}

.app_circle_right, .app_circle_right_child {
  clip: rect(0, auto, auto, 44rpx);
}

.app_circle_left_child, .app_circle_left {
  clip: rect(0, 44rpx, auto, 0);
}

.app_right_mark {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  right: 0;
  top: 0;
  /* background: #ff9e29; */
  background: #d7d7d7;
  border-radius: 50%;
  clip: rect(0, auto, auto, 44rpx);
}

.app_marker {
  position: fixed;
  width: 100vh;
  height: 100vh;
  background-image: linear-gradient(0deg, #0d3d5b, #4f2e73);
}

.app_marker.block {
  height: 0rpx;
  transition: height 10s;
}

/*遮罩  */

.app_home_page_mark {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-image: linear-gradient(0deg, #0d3d5b, #4f2e73);
}

/*遮罩淡出  */

.app_hidden_home_page {
  animation: Hidden_Page 0.6s linear;
  animation-fill-mode: forwards;
  animation-delay: 100ms;
}

@keyframes Hidden_Page {
  0% {
    opacity: 0;
    left: 0;
  }

  50% {
    opacity: 0;
  }

  100% {
    left: -1500rpx;
  }
}

.app_show_scale {
  animation: show_scale 0.3s linear;
  animation-fill-mode: forwards;
  animation-delay: 100ms;
}

@keyframes show_scale {
  0% {
    opacity: 0;
    transform: scale(0.5);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

.upshow {
  animation: shows 1s linear;
  animation-fill-mode: forwards;
  animation-delay: 100ms;
}

@keyframes shows {
  0% {
    opacity: 0;
    top: 0rpx;
  }

  25% {
    opacity: 1;
    top: -30rpx;
  }

  50% {
    opacity: 1;
    top: -60rpx;
  }

  85% {
    opacity: 1;
    top: -60rpx;
  }

  100% {
    opacity: 0;
    top: -60rpx;
  }
}

