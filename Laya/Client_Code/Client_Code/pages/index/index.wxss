@keyframes updown {
  100% {
    transform: translateY(10rpx);
  }
}

.save-app {
  position: fixed;
  top: 30rpx;
  right: 30rpx;
  width: 230rpx;
  height: 50rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 15rpx;
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  animation: updown 1s linear infinite alternate;
}

.save-app::after {
  content: '';
  border: 15rpx solid transparent;
  border-bottom: 15rpx solid rgba(255, 255, 255, 0.5);
  position: absolute;
  top: -28rpx;
  right: 100rpx;
}