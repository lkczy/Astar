<!--help.wxml-->
<view class="page">
  <view class='header'>
    <view class='header_up'></view>
    <image class='header_btm' src='../../imgs/comm/shang.png'></image>
  </view>

  <view class='text'>赞赏可获金币，还可以去广告哦！</view>

  <view class='item_group'>
    <view class='item_up'>
      <view class='item' data-item="{{one}}" bindtap="clickItem">
        <view class='item_num'>{{one}}</view>
        <view class='yuan'>元</view>

        <view class='bottom'>{{one_point}}</view>
      </view>
      <view class='item' data-item="{{two}}" bindtap="clickItem">
        <view class='item_num'>{{two}}</view>
        <view class='yuan'>元</view>

        <view class='bottom'>{{two_point}}</view>
      </view>
      <view class='item' data-item="10" bindtap="clickItem">
        <view class='item_num'>10</view>
        <view class='yuan'>元</view>
        <view class='bottom'>3600</view>
      </view>
    </view>
    <view class='item_up'>
      <view class='item' data-item="15" bindtap="clickItem">
        <view class='item_num'>15</view>
        <view class='yuan'>元</view>
        <view class='bottom'>5500</view>
      </view>
      <view class='item' data-item="20" bindtap="clickItem">
        <view class='item_num'>20</view>
        <view class='yuan'>元</view>
        <view class='bottom'>8000</view>
      </view>
      <view class='item' data-item="50" bindtap="clickItem">
        <view class='item_num'>50</view>
        <view class='yuan'>元</view>
        <view class='bottom'>30000</view>
      </view>
    </view>
  </view>
  <view class='empty'></view>
</view>