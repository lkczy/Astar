/* 呼吸效果代码 */
@keyframes waggle {
  0% {
    transform: translateY(0);
  }

  25% {
    transform:translateY(5rpx);
  }

  50% {
    transform:translateY(10rpx);
  }

  75% {
    transform: translateY(5rpx);
  }

  100% {
    transform: translateY(0);
  }
}

.animation-waggle {
  animation: waggle 2s linear infinite;
}